      'SELECT CODE, DESCRIPTION FROM CERTIFICATE_GROUP'
      'WHERE'
      '  CODE = :CODE')
    SQLLock.Strings = (
      'SELECT NULL FROM CERTIFICATE_GROUP'
      'WHERE'
      'CODE = :Old_CODE'
      'FOR UPDATE WITH LOCK')
    SQLRecCount.Strings = (
