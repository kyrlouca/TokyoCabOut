  object SearchHawbSQL: TIBCQuery
    Transaction = ReadTrans
    SQL.Strings = (
      'Select'
      '  ha.serial_number,'
      '  ha.Hab_id,'
      '  ha.fk_Mawb_refer_number,'
      '  ma.reference_number,'
      '  ma.mawb_id'
      'from '
      '   Hawb ha, Mawb ma '
      'where '
      '  ha.Hab_id = :Habid'
      '  and ma.reference_number= ha.fk_mawb_refer_number')
    ReadOnly = True
    Left = 72
    Top = 562
    ParamData = <
      item
        DataType = ftString
        Name = 'Habid'
        ParamType = ptInput
      end>
